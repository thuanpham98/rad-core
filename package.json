{
  "name": "@radts/core",
  "version": "2.1.3",
  "type": "module",
  "license": "MIT",
  "maintainers": [
    "pmttmp24@gmail.com"
  ],
  "keywords": [
    "thuanpm98",
    "@radts/core",
    "@radts/react"
  ],
  "author": "Phạm Minh Thuận <pmttmp24@gmail.com> (https://github.com/thuanpham98)",
  "homepage": "https://github.com/thuanpham98/rad-core",
  "files": [
    "dist",
    "dist/*.css"
  ],
  "main": "./dist/radts-core.cjs",
  "module": "./dist/radts-core.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/radts-core.js",
      "require": "./dist/radts-core.cjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "scripts": {
    "build": "tsc && prettier --write src/ && eslint src/ && vite build"
  },
  "devDependencies": {
    "tslib": "^2.5.0",
    "@types/node": "^20.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^3.6.0"
  },
  "description": "This library aims to create a hybrid between framework and library, providing faster and better quality web building tools.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thuanpham98/rad-core.git"
  },
  "bugs": {
    "url": "https://github.com/thuanpham98/rad-core/issues"
  }
}

