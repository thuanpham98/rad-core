{
  "name": "@rad/rad-ts",
  "version": "1.1.3",
  "type": "module",
  "license": "MIT",
  "maintainers": ["pmttmp24@gmail.com","tranrd@unicloud.com.vn"],
  "homepage": "https://git.sunshinetech.vn/research-team/common/front-end/radts",
  "files": [
    "dist",
    "dist/*.css"
  ],
  "publishConfig": {
    "@rad:registry": "https://git.sunshinetech.vn/api/v4/projects/1621/packages/npm/"
  },
  "main": "./dist/rad-ts.cjs",
  "module": "./dist/rad-ts.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rad-ts.js",
      "require": "./dist/rad-ts.cjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "npx prettier --write ./src"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.2",
    "i18next": "^22.4.11",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-svgr": "^3.2.0"
  }
}